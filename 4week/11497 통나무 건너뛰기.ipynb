    "\n",
    "for i in range(n):\n",
    "    m=int(input())\n",
    "    trees=list(map(int,input().split()))\n",
    "    trees.sort()\n",
    "    result=0\n",
    "    for j in range(2,m):\n",
    "        result=max(result,abs(trees[j]-trees[j-2]))\n",
    "    print(result)"
