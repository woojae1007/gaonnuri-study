    "import heapq\n",
    "\n",
    "n=int(input())\n",
    "L1=[]\n",
    "L2=[]\n",
    "\n",
    "for i in range(n):\n",
    "    p,d = map(int, input().split())\n",
    "    L1.append([p,d])\n",
    "L1.sort(key=lambda x:x[1])\n",
    "\n",
    "for i in L1:\n",
    "    heapq.heappush(L2, i[0])\n",
    "    if len(L2)>i[1]:\n",
    "        heapq.heappop(L2)\n",
    "print(sum(L2))"
   ]
  }
