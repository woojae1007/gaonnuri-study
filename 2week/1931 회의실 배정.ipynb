    "N=int(input())\n",
    "L1=[]\n",
    "\n",
    "for i in range(N):\n",
    "    m,k = map(int, input().split())\n",
    "    L1.append([m,k])\n",
    "L1.sort(key=lambda x:x[0])\n",
    "L1.sort(key=lambda x:x[1])\n",
    "end = 0\n",
    "count = 0\n",
    "for i,j in L1:\n",
    "    if i>=end:\n",
    "        count+=1\n",
    "        end = j\n",
    "print(count)"
   ]
