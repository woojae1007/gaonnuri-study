   "source": [
    "n = int(input())\n",
    "L1 = [0]*(n+1)\n",
    "\n",
    "for i in range(2, n+1):\n",
    "    L1[i] = L1[i-1] + 1\n",
    "    if i % 2 == 0:\n",
    "        L1[i] = min(L1[i], L1[i//2]+1)\n",
    "    if i % 3 == 0:\n",
    "        L1[i] = min(L1[i], L1[i//3] + 1)\n",
    "print(L1[n])"
   ]
  }
