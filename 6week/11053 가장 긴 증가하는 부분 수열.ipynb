    "n = int(input())\n",
    "L1 = list(map(int, input().split()))\n",
    "L2 = [0 for i in range(n)]\n",
    "for i in range(n):\n",
    "    for j in range(i):\n",
    "        if L1[i] > L1[j] and L2[i] < L2[j]:\n",
    "            L2[i] = L2[j]\n",
    "    L2[i] += 1\n",
    "print(max(L2))"
   ]
